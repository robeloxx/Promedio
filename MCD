
#include<iostream>
using namespace std ;
#define N 10
int array[N] = {10,20,30,45,55,60,75,85,90,15};

int mcd(int *a,int size){
 int maxd,r;

 for(int i=0;i<size;i++){
   cout<<a[i]<<endl;
     if(i==0){
   maxd=a[i];
  }
  do{
   r=maxd%a[i];
   maxd=a[i];
   a[i]=r;
  }while(r!=0);
 }
 cout<<"El MCD es :"<<maxd;

 
 return maxd;
}
int main(){
 mcd(array, N);
  return 0;
}
